{
  "expo": {
    "name": "MoviesNow",
    "slug": "tRPC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "movies",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.roystondz.tRPC",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.roystondz.tRPC"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png",
      "manifest": {
        "short_name": "MoviesNow",
        "display": "standalone",
        "start_url": ".",
        "background_color": "#ffffff",
        "theme_color": "#000000",
        "description": "Watch your favorite movies now!",
        "icons": [
          {
            "src": "./assets/images/logo.png",
            "sizes": "512x512",
            "type": "image/png"
          }
        ]
      },
      "serviceWorker": {
        "src": "service-worker.js"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5fdb7e56-39e1-4111-aebd-51176cd539b2"
      }
    }
  }
}
